{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "object-shorthand": [2, "consistent"]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    }
}